{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/landing", "destination": "/landing.html" },
    { "source": "/landing/", "destination": "/landing.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/landing", "dest": "/landing.html" },
    { "src": "/landing/", "dest": "/landing.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 